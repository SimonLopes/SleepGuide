{
  "expo": {
    "name": "SleepGuide",
    "slug": "SleepGuide",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.simon.SleepGuide"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.simon.SleepGuide"
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "plugins": [
      "@bacons/apple-targets"
    ]
  }
}
